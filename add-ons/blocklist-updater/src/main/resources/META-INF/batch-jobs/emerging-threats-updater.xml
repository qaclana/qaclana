<?xml version="1.0" encoding="UTF-8"?>
<job id="emerging-threats-updater" xmlns="http://xmlns.jcp.org/xml/ns/javaee" version="1.0">
    <listeners>
        <listener ref="batchToCdiListener"/>
    </listeners>
    <step id="exchangeRateSingleStep">
        <chunk retry-limit="50">
            <reader ref="emergingThreatsReader"/>
            <processor ref="emergingThreatsProcessor"/>
            <writer ref="blocklistedIpWriter"/>
        </chunk>
        <end on="COMPLETED"/>
    </step>
</job>
